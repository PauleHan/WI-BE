{
  "connection": "default",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "photoUrl": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "phone": {
      "type": "string"
    },
    "articles": {
      "collection": "articles",
      "via": "user"
    },
    "keyMemo": {
      "type": "string"
    },
    "keyOwner": {
      "type": "string"
    },
    "keyPosting": {
      "type": "string"
    },
    "keyActive": {
      "type": "string"
    },
    "comments": {
      "collection": "comments",
      "via": "user"
    }
  },
  "collectionName": "users-permissions_user"
}